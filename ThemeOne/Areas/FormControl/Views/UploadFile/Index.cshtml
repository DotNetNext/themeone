@{
    ViewBag.Title = "上传文件";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section js{
    <script src="/areas/formcontrol/views/_js/upload_file.js" type="text/javascript"></script>
}
<table>
    <tr>
        <td>
            上传图片
        </td>
        <td>
            <div id="jqxImgUpload">
            </div>
            <img src="" />
            <input type="hidden" id="imgPath" name="imgPath" />
        </td>
    </tr>
    <tr>
        <td>
            上传附件
        </td>
        <td>
          <div id="jqxFileUpload">
            </div>
            <a id="aFile"></a>
            <input type="hidden" id="filePath" name="filePath" />
        </td>
    </tr>
</table>
